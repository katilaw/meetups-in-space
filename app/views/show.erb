<h1><%= @meetup.event_name %></h1>
<ul>
  <li>Description: <%= @meetup.description %></li>
  <li>Location: <%= @meetup.location%></li>
  <li>Creator: <%= @creator%></li>
</ul>

<h2>Current Members:</h2>
  <ul>
    <% @members.each do |member| %>
      <li><%= member.user.username %></li>
      <img src="<%=member.user.avatar_url%>" alt="<%= member.user.id %>" />
    <% end %>
  </ul>

<form class="join" action="/meetups/<%= @meetup.id %>" method="post">
  <button name="join">Join Meetup</button>
</form>
